<AnchorPane id="AnchorPane" prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="logindatabase.SpMIController">
   <children>
      <VBox prefHeight="441.0" prefWidth="684.0" spacing="15.0">
         <children>
            <ImageView fitHeight="30.0" fitWidth="30.0" onMouseClicked="#exit" pickOnBounds="true" preserveRatio="true" style="-fx-cursor: hand;">
               <image>
                  <Image url="@../img/3.jpg" />
               </image>
            </ImageView>
            <VBox maxHeight="-Infinity" prefHeight="33.0" prefWidth="1097.0" spacing="10.0">
               <children>
                  <Label styleClass="lbl-select-year" text="2010 is the  Year to analysis">
                     <VBox.margin>
                        <Insets left="90.0" />
                     </VBox.margin>
                  </Label>
               </children>
            </VBox>
            <HBox alignment="CENTER_LEFT" prefHeight="559.0" prefWidth="1160.0" spacing="40.0" VBox.vgrow="ALWAYS">
               <children>
                  <VBox prefHeight="522.0" prefWidth="321.0">
                     <children>
                        <Label styleClass="lbl-select-year" text="Math list" />
                        <ListView fx:id="math" prefHeight="499.0" prefWidth="234.0" />
                     </children>
                  </VBox>
                  <VBox prefHeight="522.0" prefWidth="330.0">
                     <children>
                        <Label styleClass="lbl-select-year" text="info list" />
                        <ListView fx:id="info" prefHeight="522.0" prefWidth="364.0" />
                     </children>
                  </VBox>
                  <VBox prefHeight="522.0" prefWidth="321.0">
                     <children>
                        <Label styleClass="lbl-select-year" text="Etd a le choix" />
                        <ListView fx:id="std_ch" prefHeight="499.0" prefWidth="234.0" />
                     </children>
                  </VBox>
               </children>
            </HBox>
            <StackPane maxHeight="-Infinity" maxWidth="-Infinity" onMouseClicked="#next" styleClass="cards-level-box">
               <children>
                  <Label styleClass="cards-level-title" text="Next" />
               </children>
            </StackPane>
            <StackPane maxHeight="-Infinity" maxWidth="-Infinity" onMouseClicked="#pres" styleClass="cards-level-box">
               <children>
                  <Label styleClass="cards-level-title" text="Pres" />
               </children>
            </StackPane>
         </children>
         <padding>
            <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
         </padding>
      </VBox>
   </children>
